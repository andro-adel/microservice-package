{
    "name": "andro-adel/microservice-package",
    "description": "initialize packages required for logging , messaging with rabbitmq , redis caching",
    "type": "libraries",
    "version": "0.1.1",
    "license": "MIT",
    "require": {
        "itspire/monolog-loki": "^2.1",
        "monolog/monolog": "^3.5",
        "open-telemetry/opentelemetry-logger-monolog": "^1.0",
        "predis/predis": "^2.2",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Andro\\MicroservicePackage\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andro Adel",
            "email": "info@dd.sa"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
